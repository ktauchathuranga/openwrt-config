
config system
	option hostname 'OpenWrt'
	option timezone 'UTC'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option compat_version '1.1'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'

config gpio_switch 'lt72_power'
	option name 'Power LTE modem'
	option gpio_pin 'lt72_power'
	option value '1'

config led
	option name 'lan-act'
	option sysfs 'blue:modem'
	option trigger 'netdev'
	option dev 'lan1'
	list mode 'tx'
	list mode 'rx'

config led
	option name 'wifi'
	option sysfs 'blue:wifi'
	option trigger 'netdev'
	option dev 'phy0-ap0'
	list mode 'tx'
	list mode 'rx'

